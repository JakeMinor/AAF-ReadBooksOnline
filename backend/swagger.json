{
  "swagger": "2.0",
  "security": [{
    "cookie": []
  }],
  "securityDefinitions": {
    "cookie": {
      "name": "access_token",
      "in": "header",
      "type": "apiKey",
      "description": "JWT Authorisation header"
    }
  },
  "info": {
    "title": "ReadBooks Online API",
    "version": "1.0.0",
    "description": "This API is used for the ReadBooks Online website, this has been made as a part for the Applications and Frameworks module."
  },
  "host": "localhost:3000",
  "basePath": "/",
  "tags": [
    {
      "name": "Requests",
      "description": "Manage book requests."
    },
    {
      "name": "Users",
      "description": "Manage users"
    }
  ],
  "paths": {
    "/bookRequest": {
      "get": {
        "tags": ["Requests"],
        "summary": "Get all requests in the system. Requires authentication with any role",
        "parameters": [
          {
            "in": "query",
            "name": "bookName",
            "description": "Filter results down by the Books Name.",
            "type": "string"
          },
          {
            "in": "query",
            "name": "bookType",
            "description": "Filter results down by the Books Type.",
            "type": "array",
            "enum": ["Book", "Audiobook"]
          },
          {
            "in": "query",
            "name": "isbn",
            "description": "Filter results down by the ISBN Number.",
            "type": "string"
          },
          {
            "in": "query",
            "name": "author",
            "description": "Filter results down by the Authors Name.",
            "type": "string"
          },
          {
            "in": "query",
            "name": "requestedDateTime",
            "description": "Filter results down by the Requests date and time.",
            "type": "string"
          },
          {
            "in": "query",
            "name": "requestedBy",
            "description": "Filter results down by the ID of Client who created the request.",
            "type": "string"
          },
          {
            "in": "query",
            "name": "assignedTo",
            "description": "Filter results down by the ID of Employee who is dealing with the request.",
            "type": "string"
          },
          {
            "in": "query",
            "name": "status",
            "description": "Filter results down by the status of the request.",
            "type": "string"
          },
          {
            "in": "query",
            "name": "price",
            "description": "Filter results down by the price of the request.",
            "type": "string"
          },
          {
            "in": "query",
            "name": "limit",
            "description": "Filter the amount of items which are included on a page.",
            "type": "string",
            "default": 10
          },
          {
            "in": "query",
            "name": "offset",
            "description": "Filter the amount of items to skip.",
            "type": "string",
            "default": 0
          }
        ],
        "responses": {
          "200": {
            "description": "OK",
            "schema": {
              "$ref": "#/definitions/Requests"
            }
          },
          "401": {
            "description": "UNAUTHORISED",
            "schema": {
              "type": "string",
              "example": "The provided token is invalid or has expired."
            }
          },
          "400": {
            "description": "BAD REQUEST",
            "schema": {
              "type": "string",
              "example": "Could not convert value to ObjectId."
            }
          }
        }
      },
      "post": {
        "tags": ["Requests"],
        "summary": "Creates a new request. Requires authentication with any role",
        "parameters": [
          {
            "name": "request",
            "in": "body",
            "description": "Request which is to be created.",
            "schema": {
              "$ref": "#/definitions/CreateRequest"
            }
          }
        ],
        "produces": ["application/json"],
        "responses": {
          "201": {
            "description": "CREATED"
          },
          "400": {
            "description": "BAD REQUEST",
            "schema": {
              "type": "string",
              "example": "Data was missing or invalid."
            }
          },
          "401": {
            "description": "UNAUTHORISED",
            "schema": {
              "type": "string",
              "example": "The provided token is invalid or has expired."
            }
          }
        }
      }
    },
